<h1>My view on all my plans</h1>
<div class="container">
  <% @plans.each do |plan| %>
    <div class="intro">
      <h2 class="p-3"><%= plan.name %></h2>
      <p class="p-3"> 📝description: <%= plan.description %></br>
      📅start date: <%= plan.start_date %></br>
      📅end date: <%= plan.end_date %></p>
      <% end %>
    </div>
  </div>
</div>
